{
  "profiles": {
    "Launch Neos": {
      "commandName": "Executable",
      "executablePath": "$(NeosPath)Neos.exe",
      "commandLineArgs": "-Screen -Invisible -LoadAssembly \"Libraries/NeosModLoader.dll\" -DontAutoOpenCloudHome -Config Config.json",
      "workingDirectory": "$(NeosPath)"
    },
    "Launch Neos in VR": {
      "commandName": "Executable",
      "executablePath": "$(NeosPath)Neos.exe",
      "commandLineArgs": "-Invisible -LoadAssembly \"Libraries/NeosModLoader.dll\" -DontAutoOpenCloudHome -Config Config.json",
      "workingDirectory": "$(NeosPath)"
    },
    "Launch Neos Alt": {
      "commandName": "Executable",
      "executablePath": "$(NeosPath)Neos.exe",
      "commandLineArgs": "-Screen -Invisible -LoadAssembly \"Libraries/NeosModLoader.dll\" -DontAutoOpenCloudHome -Config Config.json -DataPath \"$(NeosAltPath)\\Data\" -CachePath \"$(NeosAltPath)\\Cache\"",
      "workingDirectory": "$(NeosPath)"
    },
    "Launch Neos in VR Alt": {
      "commandName": "Executable",
      "executablePath": "$(NeosPath)Neos.exe",
      "commandLineArgs": "-Invisible -LoadAssembly \"Libraries/NeosModLoader.dll\" -DontAutoOpenCloudHome -Config Config.json -DataPath \"$(NeosAltPath)\\Data\" -CachePath \"$(NeosAltPath)\\Cache\"",
      "workingDirectory": "$(NeosPath)"
    }
  }
}